{
  "name": "@webtiers-ddd-poc/shared-application",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/application/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "pnpm --filter @webtiers-ddd-poc/shared-application run build"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/shared/application/src/**/*.ts"]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @webtiers-ddd-poc/shared-application run clean"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @webtiers-ddd-poc/shared-application run typecheck"
      }
    }
  },
  "tags": []
}